<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gilcemar.flordeliz">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.Principal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.CadastroCalcado" />
        <activity android:name=".view.CadastroCliente" />
        <activity android:name=".view.CadastroPedido" />
        <activity android:name=".view.CadastroLote" />
        <activity android:name="view.ListaItemLote" />
        <activity android:name=".view.CadastroListaItemLote" />
        <activity android:name=".view.CadastroItemLote"></activity>
    </application>

</manifest>